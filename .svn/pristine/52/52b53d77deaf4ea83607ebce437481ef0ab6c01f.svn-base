(function (doc, Util) {
    'use strict';

    Util.loadJs([], function () {
        initPage();
        initListeners();
    });

    function initPage() {}

    /*
     * @description 点击事件
     */
    function initListeners() {
        $('body')
            //选择早餐、午餐还是晚餐
            .on('tap', '.em-tab-selector', function () {
                $(this).addClass('em-active').siblings().removeClass('em-active');
            });
    }
})(document, window.Util);
