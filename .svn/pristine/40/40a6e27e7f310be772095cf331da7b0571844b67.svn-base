.page_bg {
    background-color: #fff;
}
.early_warning {
    padding: 23px 10px;
}
.early_warning .remind {
    font-size: 21px;
    color: #333333;
    line-height: 21px;
    font-weight: bold;
}
.early_warning .time {
    color: #666666;
    font-size: 12px;
    padding-top: 12px;
}
.hint {
    padding: 12px 10px 20px;
    line-height: 23px;
}
.personal_information {
    padding-bottom: 17px;
    /* padding: 0 10px; */
    /* box-sizing: border-box; */
}
.personal_msg_box {
    margin: 0 10px;
    margin-top: 10px;
}
.personal_msg_box img {
    width: 100%;
    height: 67px;
    display: block;
}

.retroaction {
    padding: 0 10px;
    height: 45px;
    line-height: 45px;
    color: #727272;
    font-size: 15px;
    background-color: #f5f5f5;
}
.retroaction_content {
    padding: 12px 10px 15px;
    line-height: 25px;
    color: #727272;
    background-color: #fff;
}
.retroaction_content .time {
    font-size: 15px;
    margin-top: 10px;
}