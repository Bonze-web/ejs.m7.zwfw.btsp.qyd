body,html {
    background-color: #f5f5f5;
}

.checkitem {
    padding: 0 10px;
    background-color: #fff;
    font-size: 14px;
}
.checkitem-time {
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 50px;
    border-bottom: 1px solid #e6e6e6;
}
.check-name {
    color: #666;
}

.check-result {
    border-bottom: 1px solid #e6e6e6;
}
.check-result-name {
    height: 40px;
    line-height: 40px;
}
.check-result-choose {
    display: flex;
    flex-wrap: wrap;
}
.check-result-choose div {
    padding: 0 10px;
    margin-right: 8px;
    margin-bottom: 10px;
    height: 35px;
    line-height: 35px;
    background-color: #ecf4ff;
    color: #3c80e6;
    border-radius: 2px;
}
.check-result-choose .active {
    background-color: #3c80e6;
    color: #fff;
}

.checkadd {
    padding: 10px;
    margin-top: 10px;
}
.checkadd-name {
    font-size: 14px;
}
.checkadd button {
    width: 100%;
    height: 40px;
    background-color: #3c80e6;
    border: none;
    color: #fff;
    border-radius: 4px;
}
.checkadd-list {
    margin-top: 10px;
    padding: 10px;
    background-color: #fff;
    border-radius: 4px;
}
.checkadd-list li {
    padding: 8px 0;
    border-bottom: 1px solid #e6e6e6;
}
.checkadd-list-name {
    font-size: 16px;
}
.checkadd-list-content {
    color: #666;
    font-size: 12px;
}
.savebtn {
    position: fixed;
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    bottom: 0;
    background-color: #fff;
    color: #3c80e6;;
}