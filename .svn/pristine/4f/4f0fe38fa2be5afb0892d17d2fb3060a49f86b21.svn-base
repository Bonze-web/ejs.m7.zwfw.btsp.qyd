<!DOCTYPE html>
<html lang="zh-CN">

<head>
    <meta charset="utf-8">
    <title>执法人员添加</title>
    <meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1,user-scalable=no">
    <meta name="format-detection" content="telephone=no, email=no" />
    <script>
        document.write("<script src='../../js/cssboot.js?" + Math.random() + "'></scr" + "ipt>");
    </script>
    <script>
        SrcBoot.output([
            // './css/index.css'
        ]);
    </script>

    <style>
        .oulist {
            background-color: #fff;
            margin-bottom: 60px;
        }

        .oulist-item {
            border-bottom: 1px solid #e0e0e0;
        }

        /* 底部按钮 */
        .outer {
            position: fixed;
            bottom: 0px;
            width: 100%;
        }

        .btn-container {
            display: flex;
            flex-direction: row;
            justify-content: space-around;
        }

        .save-continue-btn {
            width: 100%;
            height: 40px;
            background: #1975f5;
            color: #fff;
            border-radius: 2px;
            font-size: 17px;
            text-align: center;
            line-height: 40px;
        }
    </style>
</head>

<body>

    <!-- 列表 -->

    <div class="content">
        <ul class="oulist" id="listdata">

        </ul>

    </div>

    <!-- 模板：人员 -->

    <script type="text/template" id="listdata-person-tpl">
        {{#item}}

    
        <li class="oulist-item">
        <div class="mui-input-row mui-checkbox">
            <label>{{name}}</label>
            <input name="checkbox1" value="Item 1" type="checkbox" data-value="{{name}}" data-guid="{{rowguid}}">
        </div>
    </li>
        {{/item}}
    </script>

    <script>
        document.write("<script src='../../js/jsboot.js?" + Math.random() + "'></scr" + "ipt>");
    </script>
    <script>
        SrcBoot.output([
            './js/multiple_selection_law.js'
        ]);
    </script>
</body>

</html>