<!-- 渲染 列表 -->
<script type="text/template" id="item-template">
    <li class="checklist-item checklist-{{timestyle}}" data-dutyguid="{{rowguid}}" data-opttype="{{opttype}}" data-optguid="{{optguid}}" data-lng="{{lng}}">
        <div class="checklist-title">{{taskname}}</div>
        <div class="checklist-content">
            <div>任务编号:{{taskcode}}</div>
            <div>检查对象类型:{{opttype}}</div>
            <div>执法人员:{{lepusernames}}</div>
            <div>计划日期:{{planstartdate}}至{{planenddate}} <span class="datetips">({{warningtext}}&nbsp;剩余{{warningdate}}小时)</span> </div>
        </div>
        <div class="checklist-opbtn {{statusbtnStyle}}">
            <div id="transferbtn">移交</div>
            <div id="statusbtn">{{statusbtn}}</div>
        </div>
        <div class="checklist-oponebtn {{islockstyle}}">任务正在移交中</div>
    </li>
</script>

