<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta name="format-detection" content="telephone=no" />
    <title>自查设置</title>
    <script>
        document.write("<script src='../../js/cssboot.js?" + Math.random() + "'></scr" + "ipt>");
    </script>
    <script>
        SrcBoot.output([
            './css/intercalate_index.css'
        ]);
    </script>
</head>

<body>
    <div class="page_bg bg">
        <div class="self_inspection my_line">
            <span class="inspection_title">自查频率</span>
            <div class="compute">
                <div class="subtract_left" mysrc="1"><img src="./img/subtract_blue.png" class="subtract_blue"></div>
                <input type="number" id="mynumber" value="1">
                <div class="add_right" mysrc="0"><img src="./img/add_blue.png" class="add_blue"></div>
            </div>
        </div>
        <div class="self_inspection my_line">
            <span class="inspection_title">自查时间</span>
            <div class="time">
               <div><input type="radio" id="year" name="time" checked value="3"><label for="year">每年</label></div>
               <div><input type="radio" id="month" name="time" value="2"><label for="month">每月</label></div>
               <div><input type="radio" id="week" name="time" value="1"><label for="week">每周</label></div>
            </div>
        </div>
        <!-- 保存按钮 -->
        <div class="conserve">
            保存
        </div>
    </div>


    <script>
        document.write("<script src='../../js/jsboot.js?" + Math.random() + "'></scr" + "ipt>");
    </script>
    <script>
        SrcBoot.output([
            './js/intercalate_index.js'
        ]);
    </script>
</body>

</html>