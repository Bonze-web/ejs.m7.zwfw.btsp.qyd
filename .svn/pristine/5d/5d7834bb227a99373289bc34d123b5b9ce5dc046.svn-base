* {
    user-select: text !important;
    -webkit-user-select: text !important;
}
.em-info-author {
    margin-top: 5px;
    margin-bottom: 5px;
    font-size: 14px;
    color: #333;
}

.em-info-author span {
    color: #999;
    margin-right: 10px;
}

.em-info-title {
    font-weight: 600;
    line-height: 28px;
}

.em-info-date {
    font-size: 14px;
    color: #333;
}

.em-info-date span {
    color: #999;
    margin-right: 10px;
}

.em-list-read i {
    display: inline-block;
    width: 13px;
    height: 13px;
    background: url(../img/img_read.png) no-repeat;
    background-size: 100% 100%;
    margin-right: 7px;
    margin-top: 4px;
}

.em-info-author .em-list-read,
.em-read-num {
    float: right;
    margin-right: 0;
}

.em-read-num {
    margin-right: 0px !important;
}

.em-table-view-cell {
    padding: 10px;
}

/*正文*/

.em-content {
    font-size: 14px;
    color: #333;
    padding: 10px;
    line-height: initial;
}

.em-content {
    font-size: 14px;
    color: #333;
    overflow: hidden;
    overflow-x: scroll;
}

.em-content p {
    color: #333;
}

.mui-content {
    display: none;
    position: relative;
}

/*附件*/

.em-attach img {
    width: 36px;
    height: 36px;
    float: left;
    margin-top: 2px;
}

.em-attach-right {
    display: inline-block;
    width: calc(100% - 75px);
    float: left;
    margin-left: 10px;
}

.em-attach-name {
    font-size: 15px;
    color: #333;
}

.em-attach-size {
    color: #adadad;
    font-size: 12px;
}

.em-attach-check {
    float: right;
    display: inline-block;
    width: 20px;
    height: 20px;
    background: url(../img/img_attach_check.png) no-repeat;
    background-size: 100% 100%;
    margin-top: 11px;
}

/*反馈内容*/

/*邮件反馈*/

.em-mail-feedback-content {
    padding-bottom: 60px;
}

.em-feedback-tab {
    line-height: 40px;
    height: 40px;
    overflow: hidden;
    font-size: 15px;
    padding: 0 11px;
    background: #f5f5f5;
}

.em-feedback-tab span:first-child {
    color: #666;
    margin-bottom: 0;
}

.em-feedback-tab span:last-child {
    color: #4787e7;
    float: right;
}

.em-mail-feedback-wrapper.mui-table-view {
    width: 100%;
}

.em-mail-feedback-wrapper.mui-table-view .mui-table-view-cell.mui-active {
    background-color: #fff;
}

.em-mail-feedback-wrapper.mui-table-view .mui-table-view-cell .em-feed-photo {
    width: 30px;
    height: 30px;
    border-radius: 50%;
}

.em-mail-feedback-wrapper.mui-table-view .mui-table-view-cell .em-feedback-rightwrapper {
    float: right;
    width: calc(100% - 40px);
}

.em-feed-info {}

.em-feed-info span:first-child {
    color: #000000;
    font-weight: 800;
    margin-right: 5px;
    font-size: 16px;
}

.em-feed-info span:last-child {
    color: #666666;
    font-size: 15px;
}

.em-feed-title {
    /*overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;*/
}

.em-feed-title,
.em-feed-date {
    margin-top: 8px;
}

.em-feed-title {
    font-size: 14px;
    color: #000000;
}

.em-feed-date {
    font-size: 15px;
}

.em-feed-date span:first-child {
    color: #999999;
}

.em-feed-btn {
    float: right;
}

.em-feed-btn span {
    border-radius: 5px;
    padding: 3px 6px;
}

.em-feed-btn span:first-child {
    color: #ff0000;
    border: 1px solid #ff0000;
}

.em-feed-btn span:first-child:active {
    background-color: #ff0000;
    color: #fff;
}

.em-feed-btn span:last-child {
    color: #3c80e6;
    border: 1px solid #3c80e6;
    margin-left: 10px;
}

.em-feed-btn span:last-child:active {
    background-color: #3c80e6;
    color: #fff;
}

/*底部反馈区域*/

.em-bottom-nav {
    position: fixed;
    z-index: 10;
    right: 0;
    left: 0;
    bottom: 0;
    background: #efeff4;
    border-top: 1px solid #e0e0e0;
    padding: 9px 10px;
}

.em-bottom-nav input {
    float: left;
    width: calc(100% - 100px);
    font-size: 14px;
    margin-bottom: 0;
    height: 30px;
    border-color: #e0e0e0;
}

.em-bottom-nav .em-bottom-comment {
    float: right;
    color: #3c80e6;
    font-size: 14px;
    margin-top: 6px;
}

.em-bottom-nav .em-bottom-comment .comment i {
    display: inline-block;
    width: 19px;
    height: 15px;
    background: url(../img/img_list_comment.png) no-repeat;
    background-size: 100% 100%;
    margin-right: 5px;
    vertical-align: text-top;
}

.em-bottom-nav .em-bottom-comment .praise i {
    display: inline-block;
    width: 15px;
    height: 15px;
    background: url(../img/img_praise.png) no-repeat;
    background-size: 100% 100%;
    margin-right: 5px;
    vertical-align: text-top;
}

/*底部反馈面板*/

.em-bottom-wrapper {
    width: 100%;
}

.em-feedbackContent {
    float: left;
    width: calc(100% - 56px);
    border: 1px solid #f7f7f7;
    line-height: 28px;
    text-align: 14px;
    padding-left: 10px;
    background: white;
    color: #999;
    font-size: 14px;
    border-radius: 10px;
}

.em-feedbackContent:active {
    background: #bbb;
}

.em-bottom-comment {
    display: block;
}

/*遮罩*/

.em-mask {
    display: none;
    z-index: 20;
    margin-top: 0px;
    position: fixed;
    top: 0;
    bottom: 200px;
    left: 0;
    right: 0;
    background: #000;
    opacity: 0.3;
}

/*自定义纵向顶部菜单并设置动画*/

.em-transition-vertical {
    overflow: hidden;
    height: 0px;
    /*background: #3c80e6;*/
    transition: height 100ms linear;
    -webkit-transition-duration: height 100ms linear;
    -moz-transition: height 100ms linear;
    -o-transition-property: height 100ms linear;
}

.em-tab-bar-list.mui-table-view {
    z-index: 26;
    position: fixed;
    right: 0;
    left: 0;
    top: 0;
}

.em-tab-bar-list.mui-table-view .mui-table-view-cell {}

/*格式化表格底部线*/

.em-tab-bar-list.mui-table-view:after,
.em-tab-bar-list.mui-table-view:before {
    height: 0px;
}

.em-tab-bar-list .mui-table-view-cell img {
    width: 26px;
    height: 26px;
    vertical-align: middle;
}

.em-tab-bar-list .mui-table-view-cell span {
    margin-left: 0px;
}

.em-tab-bar-list .mui-table-view-cell.selected .em-icon {
    display: inline-block;
    width: 20px;
    height: 20px;
    background-image: url(../../schedule/img/img_sdule_selected.png);
    background-size: 100% 100%;
    vertical-align: middle;
    float: right;
    margin-right: 10px;
}

.em-tab-bar-list .mui-table-view-cell.selected span {
    color: #3c80e6;
}

/*去掉头部点击效果*/

.mui-table-view-cell.mui-active {
    background-color: white;
}

/*快捷反馈*/

/*遮罩*/

.em-mask {
    display: none;
    z-index: 22;
    margin-top: 0px;
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: #000;
    opacity: 0.3;
}

/*快捷反馈dom层*/

.em-quickfeedback-wrapper {
    display: none;
    position: absolute;
    z-index: 23;
    right: 0;
    left: 0;
    bottom: 0;
    background-color: #ffffff;
        height: 200px;
        transform: translate3d(0, 200px, 0);
        transition: all 0.3s;
}
html body .showFeed {
    transform: translate3d(0, 0, 0);
}
.em-warpper-show {
    height: 100vh;
    overflow: hidden;
}
.em-quickfeedback-content {
    /*margin: 10px 0;*/
}
.em-editor {
    height: 150px;
    position: relative;
    display: inline-block;
    overflow: scroll;
    margin-top: 2px;
}
.em-quickfeedback-content .em-action-top {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 15px;
    border-bottom: 1px solid #ededed;
}

.em-quickfeedback-content .em-action-top span {
    font-size: 16px;
    color: #333;
}

.em-quickfeedback-content textarea {
    width: 100%;
    height: 150px;
    padding: 10px 15px;
    margin-bottom: 0;
    border: none;
    overflow-y: scroll;
}

.em-quickfeedback-content button {
    float: right;
    margin-bottom: 5px;
    padding: 5px 20px;
    color: #fff;
    border: 1px solid #fff;
    border-radius: 0px;
    border-top-left-radius: 0px;
    border-top-right-radius: 0px;
    border-bottom-right-radius: 0px;
    border-bottom-left-radius: 0px;
    background-color: #3c80e6;
}

.em-quickfeedback-content button:active {
    background-color: #bbb;
}

* {
    word-break: break-all;
}

/* 底部适配iphoneX */

@media (device-width: 375px) and (device-height: 812px) and (-webkit-min-device-pixel-ratio: 3) {
    .em-bottom-nav {
        padding: 9px 10px 20px;
    }
}

/*给富文本中图片img设置样式*/

/*解决text-indent和图片缩进的问题*/

/*.em-feed-title img,
.em-content img {
	max-width: 100%;
	display: block;
	margin: 0 auto;
}*/

.mui-content video {
    width: 100%;
    height: auto;
}
/* 正文内容横向滑动 */
.mui-scroll {
    padding: 10px;
    width: auto;
}

.GoTop-box {
    z-index: 99;
}
.em-mail-feedback-content p {
    color: #000;
    font-size: 14px;
}
.em-editor {
    width: 100%;
}

#editor {
    height: 100%;
}
#editor>div {
    -webkit-overflow-scrolling: touch !important;
}
#editor p {
    color: #000;
    font-size: 16px;
    width: 100%;
}