html,
body {
    width: 100%;
    background-color: #f5f5f5;
}

.em-bg {
    padding: 10px;
}

.em-btn {
    width: 100%;
    height: 44px;
    font-size: 16px;
    line-height: 16px;
    color: #fff;
    background-color: #3c80e6;
    border-radius: 5px;
    border: none;
}

/* 列表内容 */
.em-list {
    padding: 0 15px;
    margin-top: 15px;
    background-color: #fff;
    border-radius: 4px;
}

.em-item {
    position: relative;
    z-index: 1;
    padding: 15px 0;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
}

.em-item:not(:last-child)::after {
    position: absolute;
    bottom: 0;
    z-index: 1;
    width: 100%;
    height: 1px;
    content: '';
    background-color: #e6e6e6;
}

.em-delete {
    width: 20px;
    height: 20px;
}

.em-time {
    font-size: 17px;
    font-weight: bold;
    line-height: 16px;
    color: #333;
}

.em-main {
    width: 50%;
}

.em-type {
    font-size: 17px;
    line-height: 16px;
    color: #333;
}

.em-detail {
    margin-top: 8px;
}

.em-detail-span {
    font-size: 13px;
    line-height: 16px;
    color: #999;
}

.em-rename {
    width: 17px;
    height: 16px;
}
