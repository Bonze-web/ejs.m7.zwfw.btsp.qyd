/*全局格式化样式*/

body,
.mui-content {
    background-color: #ffffff;
}

/*遮罩*/

.em-mask {
    display: none;
    z-index: 20;
    margin-top: 0px;
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: #000;
    opacity: 0.3;
}

/*自定义纵向顶部菜单并设置动画*/

.em-transition-vertical {
    overflow: hidden;
    height: 0px;
    /*background: #3c80e6;*/
    transition: height 100ms linear;
    -webkit-transition-duration: height 100ms linear;
    -moz-transition: height 100ms linear;
    -o-transition-property: height 100ms linear;
}

.em-tab-bar-list.mui-table-view {
    z-index: 21;
    position: fixed;
    right: 0;
    left: 0;
    top: 0;
}

.em-tab-bar-list.mui-table-view .mui-table-view-cell {
}

/*格式化表格底部线*/

.em-tab-bar-list.mui-table-view:after,
.em-tab-bar-list.mui-table-view:before {
    height: 0px;
}

.em-tab-bar-list .mui-table-view-cell img {
    width: 26px;
    height: 26px;
    vertical-align: middle;
}

.em-tab-bar-list .mui-table-view-cell span {
    margin-left: 10px;
}

.em-tab-bar-list .mui-table-view-cell.selected .em-icon {
    display: inline-block;
    width: 20px;
    height: 20px;
    background-image: url(../img/img_sdule_selected.png);
    background-size: 100% 100%;
    vertical-align: middle;
    float: right;
    margin-right: 10px;
}

.em-tab-bar-list .mui-table-view-cell.selected span {
    color: #3c80e6;
}

/*翻页*/
.flex {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.flexs {
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
}

.em-date {
    padding: 12px;
    background: #ffffff;
}

.em-arrowicon {
    width: 28px;
    height: 24px;
}

.em-detecon {
    font-size: 18px;
    color: #333333;
}

/*日历*/

.mui-scroll-wrapper {
    top: 49px;
}

.em-calendar {
    border-bottom: 1px solid #e0e0e0;
}

/**
 * 列表内容
 **/

.mui-table-view-cell > .mui-slider-handle {
    /*background: #f5f5f5;*/
}

.mui-table-view-cell > .mui-slider-right .mui-btn {
    margin-top: 26px;
    padding: 0 20px;
    background: #f4203b;
}

/*今天日程*/

.em-sdule-wrapper {
    overflow: hidden;
    margin-bottom: 50px;
    /*margin-top: 10px;*/
    /*border-top: 1px solid #e0e0e0;
	border-bottom: 1px solid #e0e0e0;*/
}

.em-line {
    position: absolute;
    margin-top: 15px;
    width: 1px;
    background: #d8e6fa;
}

.em-table-view {
    background: none;
}

.em-table-view .mui-table-view-cell {
    padding: 0 0 0 28px;
    background: none;
    margin-right: 10px;
    margin-bottom: 10px;
    font-size: 15px;
}

.em-table-view:after,
.em-table-view .mui-table-view-cell:after {
    height: 0;
}

.em-table-view .mui-table-view-cell.mui-active {
    background-color: transparent;
}

.em-table-view .mui-table-view-cell .content {
    font-size: 15px;
    line-height: 24px;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    word-break: break-all;
    background: #fff;
    width: 100%;
    padding: 6px;
    border-left: 1px solid #eae9e9;
    border-right: 1px solid #eae9e9;
    border-bottom: 1px solid #d5d4d4;
    border-radius: 4px;
    color: #666;
}

.em-table-view .mui-table-view-cell span {
    color: #3c80e6;
    padding-top: 5px;
    margin-bottom: 6px;
    display: inline-block;
}

/*无日程安排*/

.em-nodata.mui-table-view-cell {
    display: flex;
    height: 190px;
    background-color: #fff;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    margin-right: 0;
    padding-left: 0;
}

.em-nodata.mui-table-view-cell.mui-active {
    background-color: #fff;
}

.em-nodata.mui-table-view-cell img {
    width: 150px;
    height: 93px;
}

.em-nodata.mui-table-view-cell p {
    color: #97b3de;
    margin-top: 15px;
}

/*epoint 底部导航*/

.em-bar-tab {
    bottom: 0;
    display: flex;
    width: 100%;
    height: 50px;
    padding: 0px 0px;
    table-layout: fixed;
    border-top: 0;
    border-bottom: 0;
    -webkit-touch-callout: none;
}

.em-bar {
    position: fixed;
    z-index: 10;
    right: 0;
    left: 0;
    border-bottom: 0;
    background-color: #fff;
    -webkit-box-shadow: 0 0 1px rgba(218, 217, 217, 1);
    box-shadow: 0 0 1px rgba(218, 217, 217, 1);
    -webkit-backface-visibility: hidden;
    backface-visibility: hidden;
}

.em-bar-tab .em-tab-item.em-tab-item-today {
    color: #666666;
    width: 20%;
    border-right: 1px solid rgba(218, 217, 217, 0.3);
    padding: 3px;
    text-align: center;
    display: flex;
    justify-content: center;
    align-items: center;
}

.em-bar-tab .em-tab-item.em-tab-item-today .em-tab-label {
    display: block;
    color: #666666;
    font-size: 14px;
    line-height: 15px;
    display: none;
}

.em-bar-tab .em-tab-item.em-tab-item-write {
    color: #666666;
    width: 80%;
    text-align: center;
    height: 100%;
    line-height: 55px;
    display: flex;
    justify-content: center;
    align-items: center;
}

.em-bar-tab .em-tab-item.em-tab-item-write .em-tab-label {
    color: #3c80e6;
}

.em-bar-tab .em-tab-item .em-icon-today {
    display: inline-flex;
    width: 24px;
    height: 24px;
    background-image: url(../img/img_sdule_today.png);
    background-size: 100% 100%;
}

.em-bar-tab .em-tab-item .em-icon-write {
    /* display: flex; */
    display: inline-block;
    width: 20px;
    height: 20px;
    background: url(../img/img_sdule_write.png) no-repeat;
    background-size: 100% 100%;
    margin-right: 5px;
}

/*代理日程*/

.em-table-view-agent {
    display: none;
}

.em-table-view-agent .mui-table-view-cell:before,
.em-table-view-agent .mui-table-view-cell:after,
.em-table-view-agent.mui-table-view:before {
    height: 0px;
}

.em-table-view-agent.mui-table-view:after {
    background-color: #e2e2e2;
}

.em-table-view-agent .mui-table-view-cell > a:not(.mui-btn) {
    margin: -11px -25px;
}

.mui-navigate-right:after,
.mui-push-right:after {
    right: 15px;
    content: '\e583';
    font-size: 22px;
}

.em-agent-text,
.em-agent-name {
    font-size: 15px;
    color: #999999;
}

.em-agent-name {
    float: right;
    margin-right: 25px;
    color: #3c80e6;
}

/*日程列表颜色对应的 样式*/
.lanse {
    background-color: #d4f2ff !important;
    border-color: #d4f2ff !important;
}
.hongse {
    background-color: #fcdde1 !important;
    border-color: #fcdde1 !important;
}
.huangse {
    background-color: #ffedd7 !important;
    border-color: #ffedd7 !important;
}
.lvse {
    background-color: #dbf6e9 !important;
    border-color: #dbf6e9 !important;
}

.em-circle {
    display: inline-block;
    width: 10px;
    height: 10px;
    border-radius: 50%;
    background-color: #3c80e6;
    position: absolute;
    top: 32px;
    left: 17px;
}

.em-triangle {
    position: absolute;
    top: 15px;
    left: -9px;
    width: 0;
    height: 0;
    border-top: 7px solid transparent;
    border-bottom: 7px solid transparent;
    border-right: 10px solid #ebeffa;
}

.em-sign {
    position: relative;
    width: 85%;
    background-color: #ebeffa;
    border-radius: 5px;
    margin-left: 10%;
    margin-top: 5%;
    color: #7697c8;
    padding: 7px 5px 5px 15px;
}

.em-sign span {
    font-size: 24px;
    color: #5d78a2;
}

.em-sign p {
    font-size: 14px;
    margin-bottom: 5px;
    margin-top: 5px;
    color: #7e9ccb;
}

.em-sign b {
    background-color: #d4daec;
    z-index: 15;
}

/*置顶的样式*/

.em-sign:first-child,
.em-sign:first-child b {
    background-color: #3c80e6;
    z-index: 15;
}

.em-sign:first-child i {
    border-right: 10px solid #3c80e6;
}

.em-sign:first-child span,
.em-sign:first-child p {
    color: #ffffff;
}

.em-sdule-wrapper {
    position: relative;
    background: #ffffff;
}

.em-datecon {
    width: 83px;
    text-align: center;
    margin-right: 3px;
    margin-top: 15px;
}

.em-time {
    display: block;
    font-size: 16px;
    color: #333333;
}

.em-weekcon {
    display: block;
    font-size: 12px;
    color: #999;
}

#listdata {
    width: 100%;
}

.em-calendar-content .em-calendar-item {
    width: 14.27vw !important;
}

.minirefresh-totop {
    display: none;
}

.em-sdule-wrapper {
    margin-bottom: 0;
}

.minirefresh-wrap {
    position: relative !important;
}

.minirefresh-theme-default .downwrap-content {
    padding-top: 15px !important;
}

/* 无签到信息 */
.nodata {
    position: relative;
    top: -70px;
    z-index: 1;
    float: right;
    width: calc(100% - 80px);
}

.em-circle-nodata {
    display: inline-block;
    width: 10px;
    height: 10px;
    border-radius: 50%;
    background-color: #3c80e6;
    position: absolute;
    top: 17px;
    left: -35px;
}

.em-triangle-nodata {
    position: absolute;
    top: 15px;
    left: -9px;
    width: 0;
    height: 0;
    border-top: 7px solid transparent;
    border-bottom: 7px solid transparent;
    border-right: 10px solid #ebeffa;
}

.nodata-sign {
    position: relative;
    z-index: 1;
}

.nodata-sign::before {
    position: absolute;
    left: -30px;
    top: 0;
    z-index: 1;
    width: 1px;
    height: 45px;
    content: '';
    background-color: #d8e6fa;
}
